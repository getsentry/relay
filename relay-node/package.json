{
  "name": "sentry-test-server",
  "version": "0.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "*.node"
  ],
  "napi": {
    "name": "sentry-test-server",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin"
      ]
    }
  },
  "engines": {
    "node": ">= 10.16.0"
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "test": "node ./test.js"
  },
  "devDependencies": {
    "@napi-rs/cli": "2.0.0-beta.4",
    "prettier": "^2.4.1"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  }
}
